{
	"name": "server",
	"version": "1.0.0",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"start": "node --env-file .env --experimental-strip-types --no-warnings src/server.ts",
		"dev": "node --env-file .env --experimental-strip-types --no-warnings --watch src/server.ts",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:seed": "node --env-file .env --experimental-strip-types --no-warnings src/db/seed.ts",
		"commit": "npx git-cz"
	},
	"keywords": [],
	"author": "Caio Maciel",
	"license": "MIT",
	"description": "",
	"devDependencies": {
		"@biomejs/biome": "2.0.6",
		"@types/node": "^24.0.10",
		"drizzle-seed": "^0.3.1",
		"typescript": "^5.8.3",
		"ultracite": "5.0.32"
	},
	"dependencies": {
		"@fastify/cors": "^11.0.1",
		"@fastify/multipart": "^9.0.3",
		"@fastify/swagger": "^9.5.1",
		"@fastify/swagger-ui": "^5.2.3",
		"@google/genai": "^1.8.0",
		"cz-emoji-conventional": "^1.2.1",
		"drizzle-kit": "^0.31.4",
		"drizzle-orm": "^0.44.2",
		"fastify": "^5.4.0",
		"fastify-type-provider-zod": "^5.0.1",
		"postgres": "^3.4.7",
		"zod": "^3.25.75"
	},
	"config": {
		"commitizen": {
			"path": "./server/node_modules/cz-emoji-conventional"
		}
	}
}
